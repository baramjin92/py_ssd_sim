<ufs>
	<main_configuration>
		<host_if>UFS</host_if>
		<host_speed>Gen3x2</host_speed>
		<max_payload_size>512</max_payload_size>
		<number_of_host_queue>1</number_of_host_queue>
		<depth_of_host_queue>128</depth_of_host_queue>
		<channel>4</channel>
		<way_per_channel>1</way_per_channel>
		<nand>
			<file>nand_config.xml</file>
			<name>256gb_g3</name>
		</nand>
		<dram>
			<bandwidth>12800MB/s</bandwidth>
			<buswidth>32</buswidth>
		</dram>
		<buffer>
			<write>1MB</write>
			<read>1MB</read>		
		</buffer>								
		<blk_grp>
			<blk_manager>
				<name>meta</name>
				<number_of_way>ALL</number_of_way>
				<list_of_way>None</list_of_way>
				<start_block_no>1</start_block_no>
				<end_block_no>9</end_block_no>
				<threshold_low>1</threshold_low>
				<threshold_high>3</threshold_high>
				<cell_mode>SLC</cell_mode>
			</blk_manager>
			<blk_manager>
				<name>slc_cache</name>
				<number_of_way>ALL</number_of_way>
				<list_of_way>None</list_of_way>
				<start_block_no>10</start_block_no>
				<end_block_no>49</end_block_no>
				<threshold_low>1</threshold_low>
				<threshold_high>3</threshold_high>
				<cell_mode>SLC</cell_mode>
			</blk_manager>	
			<blk_manager>
				<name>user</name>
				<number_of_way>ALL</number_of_way>
				<list_of_way>None</list_of_way>
				<start_block_no>50</start_block_no>
				<end_block_no>100</end_block_no>
				<threshold_low>1</threshold_low>
				<threshold_high>3</threshold_high>
				<cell_mode>TLC</cell_mode>
			</blk_manager>		
		</blk_grp>
		<workload>
			<file>workload.xml</file>
			<name>normal</name>
		</workload>
	</main_configuration>
</ufs>